	<name>Java Demo Code.</name>
	<description>Java Demo Code.</description>
	
	<dependencies>
		<dependency>
		    <groupId>junit</groupId>
		    <artifactId>junit</artifactId>
		    <version>4.11</version>
		    <scope>test</scope>
		</dependency>
		
  		<dependency>
		    <groupId>com.jfinal</groupId>
